<%- model_class = AddresG -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:bomg) %>:</strong></dt>
  <dd><%= @addres_g.bomg %></dd>
  <dt><strong><%= model_class.human_attribute_name(:subj) %>:</strong></dt>
  <dd><%= @addres_g.subj %></dd>
  <dt><strong><%= model_class.human_attribute_name(:indx) %>:</strong></dt>
  <dd><%= @addres_g.indx %></dd>
  <dt><strong><%= model_class.human_attribute_name(:okato) %>:</strong></dt>
  <dd><%= @addres_g.okato %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rnname) %>:</strong></dt>
  <dd><%= @addres_g.rnname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:npname) %>:</strong></dt>
  <dd><%= @addres_g.npname %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ul) %>:</strong></dt>
  <dd><%= @addres_g.ul %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dom) %>:</strong></dt>
  <dd><%= @addres_g.dom %></dd>
  <dt><strong><%= model_class.human_attribute_name(:korp) %>:</strong></dt>
  <dd><%= @addres_g.korp %></dd>
  <dt><strong><%= model_class.human_attribute_name(:kv) %>:</strong></dt>
  <dd><%= @addres_g.kv %></dd>
  <dt><strong><%= model_class.human_attribute_name(:dreg) %>:</strong></dt>
  <dd><%= @addres_g.dreg %></dd>
  <dt><strong><%= model_class.human_attribute_name(:person_id) %>:</strong></dt>
  <dd><%= @addres_g.person_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              addres_gs_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_addres_g_path(@addres_g), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              addres_g_path(@addres_g),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
