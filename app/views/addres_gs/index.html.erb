<%- model_class = AddresG -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:bomg) %></th>
      <th><%= model_class.human_attribute_name(:subj) %></th>
      <th><%= model_class.human_attribute_name(:indx) %></th>
      <th><%= model_class.human_attribute_name(:okato) %></th>
      <th><%= model_class.human_attribute_name(:rnname) %></th>
      <th><%= model_class.human_attribute_name(:npname) %></th>
      <th><%= model_class.human_attribute_name(:ul) %></th>
      <th><%= model_class.human_attribute_name(:dom) %></th>
      <th><%= model_class.human_attribute_name(:korp) %></th>
      <th><%= model_class.human_attribute_name(:kv) %></th>
      <th><%= model_class.human_attribute_name(:dreg) %></th>
      <th><%= model_class.human_attribute_name(:person_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @addres_gs.each do |addres_g| %>
      <tr>
        <td><%= link_to addres_g.id, addres_g_path(addres_g) %></td>
        <td><%= addres_g.bomg %></td>
        <td><%= addres_g.subj %></td>
        <td><%= addres_g.indx %></td>
        <td><%= addres_g.okato %></td>
        <td><%= addres_g.rnname %></td>
        <td><%= addres_g.npname %></td>
        <td><%= addres_g.ul %></td>
        <td><%= addres_g.dom %></td>
        <td><%= addres_g.korp %></td>
        <td><%= addres_g.kv %></td>
        <td><%= addres_g.dreg %></td>
        <td><%= addres_g.person_id %></td>
        <td><%=l addres_g.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_addres_g_path(addres_g), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      addres_g_path(addres_g),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_addres_g_path,
            :class => 'btn btn-primary' %>
