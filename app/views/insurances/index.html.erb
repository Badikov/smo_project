<%- model_class = Insurance -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:ter_st) %></th>
      <th><%= model_class.human_attribute_name(:enp) %></th>
      <th><%= model_class.human_attribute_name(:ogrnsmo) %></th>
      <th><%= model_class.human_attribute_name(:erp) %></th>
      <th><%= model_class.human_attribute_name(:vizit_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @insurances.each do |insurance| %>
      <tr>
        <td><%= link_to insurance.id, insurance_path(insurance) %></td>
        <td><%= insurance.ter_st %></td>
        <td><%= insurance.enp %></td>
        <td><%= insurance.ogrnsmo %></td>
        <td><%= insurance.erp %></td>
        <td><%= insurance.vizit_id %></td>
        <td><%=l insurance.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_insurance_path(insurance), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      insurance_path(insurance),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_insurance_path,
            :class => 'btn btn-primary' %>
